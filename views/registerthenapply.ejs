    function getTimeStamp() {
      var d = new Date();
      var day = d.getDate();

      var d = new Date();
      var month = d.getMonth();

      var d = new Date();
      var year = d.getFullYear();

      var d = new Date();
      var hours = d.getHours();

      var d = new Date();
      var mins = d.getMinutes();

      var d = new Date();
      var secs = d.getSeconds();

      var datetime = day + month + year + hours + ":" + mins + ":" + secs + ":";
      console.log(datetime);

    }

    function setTimeStamp() {
      document.getElementById('timestamp').value = datetime;
     
      document.getElementById('timestamp').innerHTML = datetime;
   }
   getTimeStamp()
   setTimeStamp()
  

  </script>

